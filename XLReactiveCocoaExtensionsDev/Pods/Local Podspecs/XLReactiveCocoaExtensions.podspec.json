{
  "name": "XLReactiveCocoaExtensions",
  "version": "3.0.0",
  "license": "MIT",
  "summary": "The missing part of ReactiveCocoa for development",
  "homepage": "https://github.com/xareelee/XLReactiveCocoaExtensions",
  "authors": {
    "Kang-Yu Xaree Lee": "xareelee@gmail.com"
  },
  "source": {
    "git": "https://github.com/xareelee/XLReactiveCocoaExtensions.git",
    "tag": "3.0.0",
    "submodules": true
  },
  "requires_arc": true,
  "platforms": {
    "ios": "7.0"
  },
  "default_subspecs": [
    "Core",
    "Foundation",
    "UIKit"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "XLReactiveCocoaExtensions/*.h",
        "XLReactiveCocoaExtensions/RACExtensions/*.{h,m}"
      ],
      "dependencies": {
        "ReactiveObjC": [
          "~> 1.0"
        ]
      }
    },
    {
      "name": "Foundation",
      "source_files": "XLReactiveCocoaExtensions/Foundation/*.{h,m}",
      "frameworks": "Foundation",
      "dependencies": {
        "XLReactiveCocoaExtensions/Core": [

        ]
      }
    },
    {
      "name": "UIKit",
      "source_files": "XLReactiveCocoaExtensions/UIKit/*.{h,m}",
      "frameworks": "UIKit",
      "dependencies": {
        "XLReactiveCocoaExtensions/Core": [

        ]
      }
    }
  ]
}
